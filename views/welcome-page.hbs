<div class="container">
    <div class="container">
        <div class="container">
            <h1>Добро пожаловать в наш чат</h1>
            {{#each chats}}
                <div class="row">
                    <div class="col s12 m6">
                        <div class="card blue-grey darken-1">
                            <div class="card-content white-text">
                                <a href="/chatroom/{{this._id}}" class="card-title">{{this.name}}</a>
                            </div>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</div>
